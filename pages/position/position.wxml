

<view class="weui-cells weui-media-box weui-media-box_text">
    <view class="weui-cells__title weui-media-box__title weui-media-box__title_in-text">查看位置</view>
    <view class="weui-cell">
      <map
        style="width: 100%; height: 300px;"
        latitude="{{latitude}}"
        longitude="{{longitude}}"
        scale="{{18}}"
        markers="{{markers}}"
        enable-3D="{{enable3d}}"
        show-compass="{{showCompass}}"
        enable-zoom="{{enableZoom}}"
        enable-rotate="{{enableRotate}}"
        enable-overlooking="{{enableOverlooking}}"
        enable-scroll="{{enableScroll}}"
      >
      </map>
    </view>
</view>