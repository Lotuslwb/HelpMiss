<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">操作成功</view>
            <view class="weui-msg__desc">您已成功发布，请返回首页查看。</view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary"  bindtap="returnHome">回到首页</button>
                <button class="weui-btn" type="default" bindtap="returnPublish">继续发布</button>
            </view>
        </view>
        <view class="weui-msg__extra-area">
            <footer />
        </view>
    </view>
</view>